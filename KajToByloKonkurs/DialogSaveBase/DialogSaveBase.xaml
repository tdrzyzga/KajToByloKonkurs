<Window x:Class="KajToBylo.DialogSaveBase"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KajToBylo"
        mc:Ignorable="d"
         Title="Zapisz bazę" Height="131" Width="301" Background="{DynamicResource {x:Static SystemColors.GradientInactiveCaptionBrushKey}}" WindowStartupLocation="CenterScreen">
    <StackPanel Height="93" Name="stackPanel1" Width="280">
        <StackPanel Height="55" Name="stackPanel2" Width="280" VerticalAlignment="Bottom" HorizontalAlignment="Left">
            <TextBlock x:Name="textBlock1" Text="Zapisać bazę?" TextAlignment="Center" Padding="2" Margin="0,16" HorizontalAlignment="Center" VerticalAlignment="Bottom" FontSize="14" />
        </StackPanel>
        <StackPanel Height="38" Name="stackPanel3" Width="174" Orientation="Horizontal" FlowDirection="LeftToRight" HorizontalAlignment="Right">
            <Button Content="Ok" Height="23" Name="dialogSaveBaseOk" Width="75" Margin="3" VerticalContentAlignment="Top" />
            <Button Content="Anuluj" Height="23" Name="dialogSaveBaseCancel" Width="75" Margin="3" VerticalContentAlignment="Top"/>
        </StackPanel>
    </StackPanel>
</Window>